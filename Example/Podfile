
target 'BitmovinAnalyticsCollector_Example' do
    use_frameworks!
    platform :ios, '9.0'
    pod 'BitmovinAnalyticsCollector', :path => '../'
    pod 'BitmovinPlayer', git: 'https://github.com/bitmovin/bitmovin-player-ios-sdk-cocoapod.git', tag: '2.35.0'
    pod 'GoogleAds-IMA-iOS-SDK', '3.9.2'
end

